{
  "manifest_version": 2,
  "name": "memory.limited",
  "description": "Quick Search in your history, bookmarks.",
  "version": "1.2",
  "permissions": ["history", "bookmarks", "tabs", "sessions", "chrome://favicon/"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self';img-src chrome://favicon data:;",
  "icons": {
    "128": "icon.png"
  }
}
